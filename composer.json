{
    "name": "buepro/typo3-pizpalue-distribution",
    "type": "typo3-cms-extension",
    "description": "This TYPO3 distribution provides a sample website using bootstrap and the extensions pizpalue and container_elements. The distribution tailors Swiss market featuring German as default language and additional translations to French, English and Finnish.",
    "keywords": [
        "TYPO3 CMS",
        "distribution",
        "bootstrap",
        "responsive",
        "frontend",
        "theme",
        "web",
        "news",
        "calendar",
        "container",
        "gridelements",
        "flux",
        "timelog"
    ],
    "homepage": "https://github.com/buepro/typo3-pizpalue_distribution",
    "authors": [
        {
            "name": "Roman BÃ¼chler",
            "email": "rb@buechler.pro",
            "homepage": "https://www.buechler.pro/",
            "role": "Developer"
        }
    ],
    "license": ["GPL-2.0-or-later"],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "buepro/typo3-bookmark-pages": "^2.0.1",
        "buepro/typo3-container-elements": "^3.0.0",
        "buepro/typo3-pizpalue": "~12.2.0",
        "buepro/typo3-timelog": "^1.7.0",
        "buepro/typo3-user-pizpalue": "^2.0.1",
        "friendsoftypo3/tt-address": "^5.2.1",
        "georgringer/eventnews": "^4.0",
        "georgringer/news": "^8.5.2",
        "svewap/ws-flexslider": "^1.5",
        "typo3/cms-base-distribution": "^10.4.3",
        "typo3/cms-indexed-search": "^10.4.20",
        "typo3/cms-lowlevel": "^10.4.20",
        "typo3/cms-recycler": "^10.4.20",
        "typo3/cms-redirects": "^10.4.20"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Buepro\\PizpalueDistribution\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "pizpalue_distribution"
        },
        "branch-alias": {
            "dev-2.1": "2.1.x-dev"
        }
    }
}
